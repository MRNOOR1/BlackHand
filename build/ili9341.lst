 200:Core/Src/ili9341.c **** /**
 201:Core/Src/ili9341.c ****  * @brief Draw line (Bresenham's algorithm)
 202:Core/Src/ili9341.c ****  * @param x0, y0: Start point
 203:Core/Src/ili9341.c ****  * @param x1, y1: End point
 204:Core/Src/ili9341.c ****  * @param color: RGB565 color
 205:Core/Src/ili9341.c ****  */
 206:Core/Src/ili9341.c **** void ILI9341_DrawLine(uint16_t x0, uint16_t y0, uint16_t x1, uint16_t y1, uint16_t color)
 207:Core/Src/ili9341.c **** {
 208:Core/Src/ili9341.c ****     int16_t dx = abs(x1 - x0);
 209:Core/Src/ili9341.c ****     int16_t dy = abs(y1 - y0);
 210:Core/Src/ili9341.c ****     int16_t sx = (x0 < x1) ? 1 : -1;
 211:Core/Src/ili9341.c ****     int16_t sy = (y0 < y1) ? 1 : -1;
 212:Core/Src/ili9341.c ****     int16_t err = dx - dy;
 213:Core/Src/ili9341.c ****     
 214:Core/Src/ili9341.c ****     while (1) {
 215:Core/Src/ili9341.c ****         ILI9341_DrawPixel(x0, y0, color);
 216:Core/Src/ili9341.c ****         
 217:Core/Src/ili9341.c ****         if (x0 == x1 && y0 == y1) break;
 218:Core/Src/ili9341.c ****         
 219:Core/Src/ili9341.c ****         int16_t e2 = 2 * err;
 220:Core/Src/ili9341.c ****         if (e2 > -dy) {
 221:Core/Src/ili9341.c ****             err -= dy;
 222:Core/Src/ili9341.c ****             x0 += sx;
 223:Core/Src/ili9341.c ****         }
 224:Core/Src/ili9341.c ****         if (e2 < dx) {
 225:Core/Src/ili9341.c ****             err += dx;
 226:Core/Src/ili9341.c ****             y0 += sy;
 227:Core/Src/ili9341.c ****         }
 228:Core/Src/ili9341.c ****     }
 229:Core/Src/ili9341.c **** }
